   <xsd:element name="ProductTemplate" type="opticks:ProductTemplate" />
   <xsd:complexType name="AoiLayer">
      <xsd:complexContent>
         <xsd:extension base="opticks:GraphicLayer">
            <xsd:attribute name="aoiSymbol" type="xsd:string" use="required" />
            <xsd:attribute name="aoiColor" use="required" type="opticks:ColorTripleType" />
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:element name="AoiLayer" type="opticks:AoiLayer" />
   <xsd:complexType name="GcpLayer">
      <xsd:complexContent>
         <xsd:extension base="opticks:Layer">
            <xsd:attribute name="symbolColor" use="required" type="opticks:ColorTripleType" />
            <xsd:attribute name="symbol" type="xsd:string" use="required" />
            <xsd:attribute name="symbolSize" type="xsd:nonNegativeInteger" use="required" />
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:element name="GcpLayer" type="opticks:GcpLayer" />
   <xsd:complexType name="PseudocolorLayer">
      <xsd:complexContent>
         <xsd:extension base="opticks:Layer">
            <xsd:sequence>
               <xsd:element name="PseudocolorClass" type="opticks:PseudocolorClass" minOccurs="0" maxOccurs="unbounded" form="qualified" />
            </xsd:sequence>
            <xsd:attribute name="symbol" type="xsd:string" use="required" />
