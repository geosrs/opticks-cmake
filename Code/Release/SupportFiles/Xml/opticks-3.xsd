            <xsd:attribute name="symbolSize" use="required" type="xsd:nonNegativeInteger" />
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:element name="TiePointLayer" type="opticks:TiePointLayer" />
   <xsd:complexType name="Branding">
      <xsd:sequence>
         <xsd:element name="SplashScreen" minOccurs="1" maxOccurs="1" form="qualified">
            <xsd:complexType>
               <xsd:attribute name="filePath" type="xsd:string" use="required" />
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Title" minOccurs="1" maxOccurs="1" form="qualified" type="xsd:string" />
