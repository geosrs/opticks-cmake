<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" RequiredVersion="3.0.2925.0">
   <Fragment>
      <!-- Defines the UI for the installer. -->
      <CustomAction Id="ClearMaintenanceMode" Property="__MaintenanceMode" Value="{}" />
      <CustomAction Id="LaunchOpticks" FileKey="F__Opticks" ExeCommand="" Return="asyncNoWait" />

      <UI Id="OpticksUI">
         <Binary Id="Banner_Image" SourceFile="Bitmaps/dlgbannr.bmp" />
         <Binary Id="Dialog_Side_Image" SourceFile="Bitmaps/dlgside.bmp" />
         <Binary Id="Exclamation_Icon" SourceFile="Bitmaps/exclamic.ico" />
         <Binary Id="Information_Icon" SourceFile="Bitmaps/info.ico" />
         <Binary Id="Repair_Icon" SourceFile="Bitmaps/repair.ico" />
         <Binary Id="Modify_Icon" SourceFile="Bitmaps/modify.ico" />
         <Binary Id="Remove_Icon" SourceFile="Bitmaps/remove.ico" />
         <Binary Id="New_Folder_Image" SourceFile="Bitmaps/New.ico" />
         <Binary Id="Up_Folder_Image" SourceFile="Bitmaps/Up.ico" />

         <Property Id="DefaultUIFont" Value="Normal_Font" />
         <Property Id="__BrowseDir" Value="TARGETDIR" />
         <!-- Default value to use in the MsiRMFilesInUse radio button control -->
         <Property Id="__RestartManagerOption" Value="UseRM" />
         <!-- Name of dialog that Windows Installer should use to display errors -->
         <Property Id="ErrorDialog" Value="InstallErrorDlg" />
         <Property Id="__UserSelection" Value="Everyone" />
         <Property Id="__MaintenanceMode" Value="Change" />
         <Property Id="__LicenseAgreement" Value="No" />
         <Property Id="RUNOPTICKS" Value="1" />

         <TextStyle Id="Normal_Font" FaceName="Tahoma" Size="8" />
         <TextStyle Id="Emphasized_Font" FaceName="Verdana" Size="13" Bold="yes" />
         <TextStyle Id="Title_Font" FaceName="Tahoma" Size="8" Bold="yes" />

         <?define DialogTitle = "[ProductName] Setup" ?>
         <?define BackButtonText = "< &Back" ?>
         <?define NextButtonText = "&Next >" ?>
         <?define CancelButtonText = "Cancel" ?>
         <?define OkButtonText = "OK" ?>

         <InstallUISequence>
            <Custom Action="ClearMaintenanceMode" Before="AppSearch">NOT Installed</Custom>
               
            <!-- Dialog to show while starting up install -->
            <Show Dialog="StartupDlg" Before="AppSearch" />

            <!-- The initial dialog to display depending on given conditions -->
            <Show Dialog="EntryInstallDlg" Before="ProgressDlg">NOT Installed</Show>
            <Show Dialog="EntryMaintenanceDlg" Before="ProgressDlg">Installed AND NOT RESUME AND NOT Preselected</Show>
            <Show Dialog="EntryResumeDlg" Before="ProgressDlg">Installed AND (RESUME OR Preselected)</Show>

            <!-- Show modeless progress dialog before transferring control to the execute sequence -->
            <Show Dialog="ProgressDlg" Before="ExecuteAction" />

            <!-- Exit Dialogs depending on success of execute sequence -->
            <Show Dialog="ExitFatalErrorDlg" OnExit="error" />
            <Show Dialog="ExitCanceledDlg" OnExit="cancel" />
            <Show Dialog="ExitSuccessDlg" OnExit="success" />
         </InstallUISequence>

         <AdminUISequence>
            <!-- Exit Dialogs depending on success of execute sequence -->
            <Show Dialog="ExitFatalErrorDlg" OnExit="error" />
            <Show Dialog="ExitCanceledDlg" OnExit="cancel" />
            <Show Dialog="ExitSuccessDlg" OnExit="success" />
         </AdminUISequence>

         <!-- Dialogs, listed in alphabetical order -->
         <Dialog Id="CancelConfirmDlg" Y="10" Width="260" Height="85" Title="$(var.DialogTitle)">
            <!-- This dialog will confirm if the user's wishes to cancel and exit the installer. -->
            <Control Id="No" Type="PushButton" X="132" Y="57" Width="56" Height="17" Default="yes" Cancel="yes" Text="&amp;No">
               <Publish Event="EndDialog" Value="Return">1</Publish>
            </Control>
            <Control Id="Yes" Type="PushButton" X="72" Y="57" Width="56" Height="17" Text="&amp;Yes">
               <Publish Event="EndDialog" Value="Exit">1</Publish>
            </Control>
            <Control Id="Text" Type="Text" X="48" Y="15" Width="194" Height="30" Text="Are you sure you want to cancel [ProductName] installation?" />
            <Control Id="Icon" Type="Icon" X="15" Y="15" Width="24" Height="24" ToolTip="Information icon" FixedSize="yes" IconSize="32" Text="Information_Icon" />
         </Dialog>

         <Dialog Id="ConfirmChangeDlg" Width="370" Height="270" Title="$(var.DialogTitle)" TrackDiskSpace="yes">
            <Control Id="Change" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="&amp;Change">
               <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="SpawnDialog" Value="OutOfRollbackDiskSpaceDlg">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
               <Publish Event="EndDialog" Value="Return">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="EnableRollback" Value="False">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="SpawnDialog" Value="OutOfDiskSpaceDlg">(OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 1) OR (OutOfDiskSpace = 1 AND PROMPTROLLBACKCOST="F")</Publish>
            </Control>
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Click Change to modify the current installation." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="300" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Confirm installation change" />
            <Control Id="Text" Type="Text" X="11" Y="56" Width="349" Height="80" Text="Click Change to begin the installation. Click Back to review or change any of your installation settings. Click Cancel to exit the wizard." />
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="$(var.BackButtonText)">
               <Publish Event="NewDialog" Value="FeatureSelectionDlg">1</Publish>
            </Control>
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
         </Dialog>

         <Dialog Id="ConfirmInstallDlg" Width="370" Height="270" Title="$(var.DialogTitle)" TrackDiskSpace="yes">
            <Control Id="Install" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="&amp;Install">
               <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="SpawnDialog" Value="OutOfRollbackDiskSpaceDlg">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
               <Publish Event="EndDialog" Value="Return">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="EnableRollback" Value="False">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="SpawnDialog" Value="OutOfDiskSpaceDlg">(OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 1) OR (OutOfDiskSpace = 1 AND PROMPTROLLBACKCOST="F")</Publish>
            </Control>
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Click Install to begin the installation." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="300" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Confirm installation" />
            <Control Id="Text" Type="Text" X="11" Y="56" Width="349" Height="80" Text="Click Install to begin the installation. Click Back to review or change any of your installation settings. Click Cancel to exit the wizard." />
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="$(var.BackButtonText)">
               <Publish Event="NewDialog" Value="FeatureSelectionDlg">1</Publish>
            </Control>
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
         </Dialog>

         <Dialog Id="ConfirmRemoveDlg" Width="370" Height="270" Title="$(var.DialogTitle)" TrackDiskSpace="yes">
            <Control Id="Remove" Type="PushButton" X="236" Y="243" Width="56" Height="17" Text="&amp;Remove">
               <Publish Event="Remove" Value="All"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="SpawnDialog" Value="OutOfRollbackDiskSpaceDlg">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
               <Publish Event="EndDialog" Value="Return">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="EnableRollback" Value="False">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="SpawnDialog" Value="OutOfDiskSpaceDlg">(OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 1) OR (OutOfDiskSpace = 1 AND PROMPTROLLBACKCOST="F")</Publish>
            </Control>
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Click Remove to remove the product." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="300" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Confirm [ProductName] removal" />
            <Control Id="Text" Type="Text" X="11" Y="56" Width="349" Height="80" Text="Click Remove to remove [ProductName] from your computer. Click Back to review or change any of your installation settings. Click Cancel to exit the wizard." />
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="$(var.BackButtonText)">
               <Publish Event="NewDialog" Value="MaintenanceTypeDlg">1</Publish>
            </Control>
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
         </Dialog>

         <Dialog Id="ConfirmRepairDlg" Width="370" Height="270" Title="$(var.DialogTitle)" TrackDiskSpace="yes">
            <Control Id="Repair" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="Re&amp;pair">
               <Publish Event="ReinstallMode" Value="ecmus"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="Reinstall" Value="All"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="SpawnDialog" Value="OutOfRollbackDiskSpaceDlg">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
               <Publish Event="EndDialog" Value="Return">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="EnableRollback" Value="False">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="SpawnDialog" Value="OutOfDiskSpaceDlg">(OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 1) OR (OutOfDiskSpace = 1 AND PROMPTROLLBACKCOST="F")</Publish>
            </Control>
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Click Repair to repair the current installation." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="300" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Confirm installation repair" />
            <Control Id="Text" Type="Text" X="11" Y="56" Width="349" Height="80" Text="Click Repair to repair the installation of [ProductName]. Click Back to review or change any of your installation settings. Click Cancel to exit the wizard." />
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="$(var.BackButtonText)">
               <Publish Event="NewDialog" Value="MaintenanceTypeDlg">1</Publish>
            </Control>
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
         </Dialog>

         <Dialog Id="DestinationBrowseDlg" Width="370" Height="270" Title="$(var.DialogTitle)">
            <!-- This dialog allows the user to browse for a directory.  It will modify the property indirectly referenced in the _BrowseProperty property.
            This dialog is meant to be shown using the SpawnDialog control event. -->
            <Control Id="PathEdit" Type="PathEdit" X="68" Y="202" Width="292" Height="18" Property="_BrowseProperty" Indirect="yes" />
            <Control Id="OK" Type="PushButton" X="240" Y="243" Width="56" Height="17" Default="yes" Text="$(var.OkButtonText)">
               <Publish Event="SetTargetPath" Value="[_BrowseProperty]">1</Publish>
               <Publish Event="EndDialog" Value="Return">1</Publish>
            </Control>
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="Reset" Value="0">1</Publish>
               <Publish Event="EndDialog" Value="Return">1</Publish>
            </Control>
            <Control Id="ComboLabel" Type="Text" X="11" Y="59" Width="33" Height="10" TabSkip="no" Text="&amp;Look in:" />
            <Control Id="DirectoryCombo" Type="DirectoryCombo" X="49" Y="56" Width="259" Height="80" Property="_BrowseProperty" Indirect="yes" Fixed="yes" Remote="yes">
               <Subscribe Event="IgnoreChange" Attribute="IgnoreChange" />
            </Control>
            <Control Id="UpButton" Type="PushButton" X="317" Y="56" Width="17" Height="17" ToolTip="Up one level" Icon="yes" FixedSize="yes" IconSize="16" Text="Up_Folder_Image">
               <Publish Event="DirectoryListUp" Value="0">1</Publish>
            </Control>
            <Control Id="NewFolder" Type="PushButton" X="341" Y="56" Width="17" Height="17" ToolTip="Create a new folder" Icon="yes" FixedSize="yes" IconSize="16" Text="New_Folder_Image">
               <Publish Event="DirectoryListNew" Value="0">1</Publish>
            </Control>
            <Control Id="DirectoryList" Type="DirectoryList" X="11" Y="83" Width="349" Height="110" Property="_BrowseProperty" Sunken="yes" Indirect="yes" TabSkip="no" />
            <Control Id="PathLabel" Type="Text" X="11" Y="205" Width="51" Height="10" TabSkip="no" Text="&amp;Folder name:" />
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Select the folder where [ProductName] should be installed." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Choose installation folder" />
         </Dialog>

         <Dialog Id="DestinationDlg" Width="370" Height="270" Title="$(var.DialogTitle)" TrackDiskSpace="yes">
            <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="$(var.NextButtonText)">
               <Publish Event="SetTargetPath" Value="[__BrowseDir]" Order="1">1</Publish>
               <Publish Event="NewDialog" Value="TempDataDlg" Order="2">1</Publish>
            </Control>
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="$(var.BackButtonText)">
               <Publish Event="NewDialog" Value="InstallForDlg">1</Publish>
            </Control>
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="DiskCost" Type="PushButton" X="111" Y="243" Width="56" Height="17" Text="Disk &amp;Usage">
               <Publish Event="SpawnDialog" Value="DiskUsageDlg">1</Publish>
            </Control>
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="Click Next to install to the default folder or click Browse to choose another." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Installation Folder" />
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
            <Control Id="FolderLabel" Type="Text" X="11" Y="56" Width="290" Height="10" Text="Install [ProductName] to:" />
            <Control Id="Folder" Type="PathEdit" X="11" Y="73" Width="349" Height="18" Property="__BrowseDir" Indirect="yes" />
            <Control Id="ChangeFolder" Type="PushButton" X="11" Y="96" Width="56" Height="17" Text="&amp;Browse...">
               <Publish Property="_BrowseProperty" Value="[__BrowseDir]" Order="1">1</Publish>
               <Publish Event="SpawnDialog" Value="DestinationBrowseDlg" Order="2">1</Publish>
            </Control>
         </Dialog>

         <Dialog Id="DiskUsageDlg" Width="370" Height="270" Title="$(var.DialogTitle)">
            <Control Id="OK" Type="PushButton" X="304" Y="243" Width="56" Height="17" Default="yes" Cancel="yes" Text="$(var.OkButtonText)">
               <Publish Event="EndDialog" Value="Return">1</Publish>
            </Control>
            <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="Banner_Image" />
            <Control Id="Text" Type="Text" X="11" Y="56" Width="349" Height="31" Text="Highlighted volumes do not have enough disk space available for selected features. You can either remove some files from the highlighted volumes, install fewer features, or select different destination drives." />
            <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
            <Control Id="Description" Type="Text" X="17" Y="22" Width="280" Height="20" Transparent="yes" NoPrefix="yes" Text="Review the requirements for the selected install features." />
            <Control Id="Title" Type="Text" X="11" Y="7" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="{\Title_Font}Disk Space Requirements" />
            <Control Id="VolumeList" Type="VolumeCostList" X="11" Y="96" Width="349" Height="124" Sunken="yes" Fixed="yes" Remote="yes" Text="{40}{70}{70}{70}{70}" />
         </Dialog>

         <Dialog Id="EntryInstallDlg" Width="370" Height="270" Title="$(var.DialogTitle)">
            <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="$(var.NextButtonText)">
               <Publish Event="NewDialog" Value="LicenseAgreementDlg">1</Publish>
            </Control>
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Bitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="234" TabSkip="no" Text="Dialog_Side_Image" />
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Disabled="yes" Text="$(var.BackButtonText)" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
            <Control Id="Description" Type="Text" X="135" Y="80" Width="220" Height="60" Transparent="yes" NoPrefix="yes" Text="The Setup Wizard will install [ProductName] on your computer. Click Next to continue or Cancel to exit the Setup Wizard." />
            <Control Id="Title" Type="Text" X="135" Y="20" Width="220" Height="60" Transparent="yes" NoPrefix="yes" Text="{\Emphasized_Font}Welcome to the [ProductName] Setup Wizard" />
         </Dialog>

         <Dialog Id="EntryMaintenanceDlg" Width="370" Height="270" Title="$(var.DialogTitle)">
            <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="$(var.NextButtonText)">
               <Publish Event="SpawnWaitDialog" Value="WaitForCostingDlg">CostingComplete = 1</Publish>
               <Publish Event="NewDialog" Value="MaintenanceTypeDlg">1</Publish>
            </Control>
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Bitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="234" TabSkip="no" Text="Dialog_Side_Image" />
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Disabled="yes" Text="$(var.BackButtonText)" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
            <Control Id="Title" Type="Text" X="135" Y="20" Width="220" Height="60" Transparent="yes" NoPrefix="yes" Text="{\Emphasized_Font}Welcome to the [ProductName] Setup Wizard" />
            <Control Id="Description" Type="Text" X="135" Y="70" Width="220" Height="60" Transparent="yes" NoPrefix="yes" Text="The Setup Wizard allows you to change the way [ProductName] features are installed on your computer or to remove it from your computer. Click Next to continue or Cancel to exit the Setup Wizard." />
         </Dialog>

         <Dialog Id="EntryResumeDlg" Width="370" Height="270" Title="$(var.DialogTitle)">
            <Control Id="Install" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="&amp;Install">
               <Publish Event="SpawnWaitDialog" Value="WaitForCostingDlg">CostingComplete = 1</Publish>
               <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace <> 1]]></Publish>
               <Publish Event="SpawnDialog" Value="OutOfRollbackDiskSpaceDlg">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
               <Publish Event="EndDialog" Value="Return">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="EnableRollback" Value="False">OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 0 AND PROMPTROLLBACKCOST="D"</Publish>
               <Publish Event="SpawnDialog" Value="OutOfDiskSpaceDlg">(OutOfDiskSpace = 1 AND OutOfNoRbDiskSpace = 1) OR (OutOfDiskSpace = 1 AND PROMPTROLLBACKCOST="F")</Publish>
            </Control>
            <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="$(var.CancelButtonText)">
               <Publish Event="SpawnDialog" Value="CancelConfirmDlg">1</Publish>
            </Control>
            <Control Id="Bitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="234" TabSkip="no" Text="Dialog_Side_Image" />
            <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Disabled="yes" Text="$(var.BackButtonText)" />
            <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
            <Control Id="Description" Type="Text" X="135" Y="80" Width="220" Height="60" Transparent="yes" NoPrefix="yes" Text="The Setup Wizard will complete the installation of [ProductName] on your computer. Click Install to continue or Cancel to exit the Setup Wizard." />
            <Control Id="Title" Type="Text" X="135" Y="20" Width="220" Height="60" Transparent="yes" NoPrefix="yes" Text="{\Emphasized_Font}Resuming the [ProductName] Setup Wizard" />
         </Dialog>

         <Dialog Id="ExitCanceledDlg" Width="370" Height="270" Title="$(var.DialogTitle)">
